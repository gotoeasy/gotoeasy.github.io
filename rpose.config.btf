[path] // 目录配置
build : build

[theme] // 样式风格包
rpose-theme-demo

[prerender] // 预渲染包
@gotoeasy/pre-render

[csslib] // 样式库配置
normalize = normalize.css: normalize.css

[svgicon]
500px = @fortawesome/fontawesome-free:500px
size-500 = @fortawesome/fontawesome-free:500px

[port] // 开发模式时的服务端口号，默认3700，非数字时随机
