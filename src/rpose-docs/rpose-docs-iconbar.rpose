[less]
.iconbar-link {
    display: flex;
    flex-direction: column;
    align-items: center;
    height: calc(100% - 50px);
    box-sizing: border-box;
    padding-top: 10px;

    & > a {
        height: 42px;
    }
    & svg {
        fill: #bbb;
        width: 28px;
        height: 28px;
    }
    & svg:hover {
        fill: #fff;
        margin-top: -2px;
        margin-left: -2px;
        width: 34px;
        height: 34px;
    }
}
